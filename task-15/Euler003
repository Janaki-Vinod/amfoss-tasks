########Largest Prime Factor#####
   
import math
n=int(input())
for i in range(n):
    l=[]
    d=int(input())
    for h in range (1,d+1):
        if d%h==0:
            p=0
            for j in range(2,h):
                if h%j==0:
                    p=j
            if p==0:
                l.append(h)
    print(max(l))
math.ceil(math.sqrt(d))


